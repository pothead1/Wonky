  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/potgrowhub/potgrowhub.git"
  },
  "bugs": {
    "url": "https://github.com/potgrowhub/potgrowhub/issues"
  },
  "homepage": "https://www.potgrowhub.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:ai-worker\"",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:frontend": "vite --config src/frontend/vite.config.js",
    "dev:ai-worker": "nodemon src/ai-core/worker.js",
    "build": "npm run clean && npm run build:backend && npm run build:frontend && npm run build:ai-core",
    "build:backend": "babel src/backend -d dist/backend --extensions \".js,.ts\"",
    "build:frontend": "vite build --config src/frontend/vite.config.js",
    "build:ai-core": "babel src/ai-core -d dist/ai-core --extensions \".js,.ts\"",
    "clean": "rimraf dist coverage",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx,json,md}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:rollback": "knex migrate:rollback",
    "deploy:prod": "npm run build && gcloud app deploy",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
